type Card {
    id:ID!
    balance:Float!
    accountId:ID!
    number:String!
}

type PaymentResultStatus {
    status:Boolean!
}

type Mutation {
    createUserTransaction(cardId:ID!,toCard:String!):PaymentResultStatus
    createFeePayment(cardId:ID!,toAccount:String!):PaymentResultStatus
}

type Query {
    getUserCards:[Card]
}