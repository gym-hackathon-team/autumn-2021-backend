type Card{
    id:ID!
    balance:Float!
    accountId:ID!
    number:String!
}

type PaymentResultStatus{
    status:Boolean!
}

type Mutation {
    createUserTransaction(id:ID!,cardId:ID!,toCard:String!):PaymentResultStatus
    createFeePayment(id:ID!,cardId:ID!,toAccount:ID!):PaymentResultStatus
}

type Query {
    getUserCards(id:ID!):[Card]
}